package com.bjsxt.pojo;

/**
 * 选课实体类
 * @author 997
 *
 */
public class Choose {
	/**
	 * 课程的id
	 */
	private int subid;
	/**
	 * 学生的id
	 */
	private int sid;
	/**
	 * 学生的成绩
	 */
	private double score;
	/**
	 * 老师的id
	 */
	private int tid;
	public int getSubid() {
		return subid;
	}
	public void setSubid(int subid) {
		this.subid = subid;
	}
	public int getSid() {
		return sid;
	}
	public void setSid(int sid) {
		this.sid = sid;
	}
	public double getScore() {
		return score;
	}
	public void setScore(double score) {
		this.score = score;
	}
	public int getTid() {
		return tid;
	}
	public void setTid(int tid) {
		this.tid = tid;
	}
	public Choose(int subid, int sid, double score, int tid) {
		super();
		this.subid = subid;
		this.sid = sid;
		this.score = score;
		this.tid = tid;
	}
	public Choose() {
		super();
	}
	@Override
	public String toString() {
		return "Choose [subid=" + subid + ", sid=" + sid + ", score=" + score
				+ ", tid=" + tid + "]";
	}
	
	
	
}
